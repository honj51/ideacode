    <TD align=left width=574>
      <FORM id=uploadFileForm name=uploadFileForm action="uploadFile.aspx" method=post encType=multipart/form-data>
        <INPUT class=text id=myFile onkeydown="return false;" type=file size=12 name=myFile> 
        <INPUT class=btn id=Button1 onclick="javascript:return CheckUpload();" type=submit value=传送 name=Button1> 
        <INPUT class=btn onclick=parent.globalChatHandle.hideUploadFileForm(); type=button value=关闭 name=Submit2> 
      </FORM>
    </TD></TR>
