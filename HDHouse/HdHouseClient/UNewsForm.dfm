  object qry_News: TADOQuery
    Connection = HDHouseDataModule.con1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT d.DeptName,e.EmpName,n.NewsID,n.NewsType,n.Title,Content,' +
        'n.Source,n.EmpID,n.RegDate,n.ModDate,n.FlagTrashed,n.FlagDeleted' +
        ',n.ExDate,n.TitleColor '
      
        'FROM News n,Employee e,Department d where n.EmpID=e.EmpID and e.' +
        'DeptID=d.DeptID and n.FlagDeleted=0 and n.FlagTrashed=0 ')
    Left = 488
